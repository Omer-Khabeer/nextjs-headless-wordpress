# Example .env file for wp-next-theme frontend

# WordPress API URL
NEXT_PUBLIC_WORDPRESS_API_URL=https://your-wordpress-site.com/wp-json

# WordPress Site URL
NEXT_PUBLIC_WORDPRESS_SITE_URL=https://your-wordpress-site.com

# Next.js Site URL
NEXT_PUBLIC_NEXT_SITE_URL=http://localhost:3000

# Google Analytics ID
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your-google-analytics-id

# Environment
NODE_ENV=development