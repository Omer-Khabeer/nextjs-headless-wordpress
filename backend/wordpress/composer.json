{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
      {
          "type": "composer",
          "url": "https://wpackagist.org"
      }
  ],
  "require": {
      "composer/installers": "~1.0",
      "imranhsayed/headless-cms": "^1.7",
      "wp-graphql/wp-graphql": "^1.3.6",
      "wp-graphql/wp-graphql-jwt-authentication": "0.4.0",
      "pristas-peter/wp-graphql-gutenberg": "0.3.7",
      "wpackagist-plugin/advanced-custom-fields": "5.9.1",
      "yoast/wordpress-seo": "^16.2",
      "ashhitch/wp-graphql-yoast-seo": "^4.14",
      "valu/wp-graphql-offset-pagination": "^0.2"
  },
  "config": {
      "allow-plugins": {
          "composer/installers": true
      }
  },
  "extra": {
      "installer-paths": {
          "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
          "wp-content/themes/{$name}/": ["type:wordpress-theme"]
      }
  }
}